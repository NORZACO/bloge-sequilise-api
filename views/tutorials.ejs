          <form id="tutorial-filter-form">
            <div class="row">
              <div class="col-md-4 mb-3">
                <label for="description" class="form-label">Search:</label>
                <input type="text" class="form-control" id="description" name="description"
                  placeholder="write description here">
              </div>
              <div class="col-md-4 mb-3">
                <label for="sort-select" class="form-label">Sort by:</label>
                <select class="form-select" id="sort" name="sort">
                  <option value="">-- Select a property --</option>
                  <option value="title" data-direction="asc">Title (A-Z)</option>
                  <option value="title" data-direction="desc">Title (Z-A)</option>
                  <option value="date" data-direction="asc">Date (oldest first)</option>
                  <option value="date" data-direction="desc">Date (newest first)</option>
                </select>
              </div>
              <div class="col-md-4 mb-3">
                <label for="page-size-select" class="form-label">Page size:</label>
                <select class="form-select" id="size" name="size">
                  <option value="">Select a number</option>
                  <option value="1">1</option>
                  <option value="2">2</option>
                  <option value="3">3</option>
                  <option value="4">4</option>
                  <option value="5">5</option>
                  <option value="6">6</option>
                  <option value="7">7</option>
                  <option value="8">8</option>
                  <option value="9">9</option>
                  <option value="10">10</option>
                </select>
              </div>
            </div>
            <div class="row">
              <div class="col-md-4 mb-3">
                <label for="page" class="form-label">Page number:</label>
                <input type="number" class="form-control" id="page" name="page" min="1" placeholder="write Page number">
              </div>
            </div>
            <button type="submit" class="btn btn-primary">Apply</button>
          </form>

        </div>
